  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Attach using Process Id",
      "type": "python",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": true
    },

    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: modul",
      "type": "python",
      "python": "${command:python.interpreterPath}",
      "request": "launch",
      "module": "demucs",
      "justMyCode": true,
      "args": [
        "--musdb",
        "/Users/dg/Documents/Development/02456_Deep_Learning/LibriMix_Sample_With_Test/Libri2Mix/wav8k/min",
        "-b",
        "4",
        "--samplerate",
        "8000",
        "--samples",
        "24000"
      ]
    },
    {
      "name": "Python: Current File 1",
      "type": "python",
      "python": "${command:python.interpreterPath}",
      "request": "launch",
      "program": "demucs/evaluate.py",
      "justMyCode": true,
      "args": [
        "--musdb",
        "/Users/dg/Documents/Development/02456_Deep_Learning/LibriMix_Sample_With_Test/Libri2Mix/wav8k/min",
        "--model_path",
        "/Users/dg/Documents/Development/02456_Deep_Learning/demucs/models/musdb=min_epochs=1_batch_size=24.th",
        "--evals",
        "/Users/dg/Documents/Development/02456_Deep_Learning/demucs/evals"
      ]
    },
    {
      "name": "Python: modul 2",
      "type": "python",
      "python": "${command:python.interpreterPath}",
      "request": "launch",
      "module": "evaluate",
      "justMyCode": true,
      "args": [
        "--audio_dir",
        "/Users/dg/Documents/Development/02456_Deep_Learning/LibriMix_Sample_With_Test/Libri2Mix/wav8k/max",
        "--model_path",
        "/Users/dg/Documents/Development/02456_Deep_Learning/demucs/demucs.th",
      ]
    }
  ]
}
